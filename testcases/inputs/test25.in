// FIFO lock, T2 is waiting for T1 and T3,
// T2 can't write until T1 and T3 both finishes
// T3 writes first, then T2 writes. Final value of x2 is 202
begin(T1)
begin(T2)
begin(T3)
R(T1, x2)
R(T3, x2)
W(T2, x2, 202)
end(T1)
W(T3, x2, 302)
end(T2)
dump()


